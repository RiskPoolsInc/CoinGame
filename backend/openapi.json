{
  "openapi": "3.0.3",
  "info": {
    "title": "Swagger CoinGame Backend - OpenAPI 3.0",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "localhost:3000"
    }
  ],
  "paths": {
    "/upload-game-wallet": {
      "get": {
        "summary": "Upload game wallet",
        "description": "Uploads a game wallet at the start of the game. If funds are left at the wallet, later they are automatically refunded.",
        "parameters": [
          {
            "name": "address",
            "in": "query",
            "description": "Wallet address",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "privateKey",
            "in": "query",
            "description": "Wallet private key",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "publicKey",
            "in": "query",
            "description": "Wallet public key",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "400": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/play-game": {
      "get": {
        "summary": "Upload game wallet",
        "description": "Uploads a game wallet at the start of the game. If funds are left at the wallet, later they are automatically refunded.",
        "parameters": [
          {
            "name": "round",
            "in": "query",
            "description": "Number of rounds",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "bid",
            "in": "query",
            "description": "Player's bid",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "gameWalletAddress",
            "in": "query",
            "description": "Game wallet address",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "gameWalletPrivateKey",
            "in": "query",
            "description": "Game wallet private key",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "gameWalletPublicKey",
            "in": "query",
            "description": "Game wallet public key",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "400": {
            "description": "Invalid input"
          }
        }
      }
    }
  }
}