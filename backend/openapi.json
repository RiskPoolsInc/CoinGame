          },
          {
            "name": "privateKey",
            "in": "query",
            "description": "Wallet private key",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "publicKey",
            "in": "query",
            "description": "Wallet public key",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "400": {
            "description": "Invalid input"
          }
        }
      }
    }
